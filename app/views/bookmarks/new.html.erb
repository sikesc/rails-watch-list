<div class="container">
  <h1>Add a Movie to your List</h1>
  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <div class="d-grid gap-2">
      <%= f.submit 'Add', class: 'btn btn-success mt-3' %>
    </div>
  <% end %>
</div>
<%= link_to "home", lists_path %>
